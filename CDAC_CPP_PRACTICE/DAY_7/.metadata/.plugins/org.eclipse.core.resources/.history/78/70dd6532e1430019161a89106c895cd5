#include<iostream>
#include<string>
class person{
private :
	string name;
	int age;
public:
	person(void)
	{
		cout<<"	person(void)"<<endl;
		this->name="";
		this->age=0
	}
	person(string name)
		{
			cout<<"	person(void)"<<endl;
			this->name="";
			this->age=0
		}
};



